{
  "action": {
    "default_icon": "./resource/assets/icons/ext-icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"]
    }
  ],
  "description": "Saving Linkedin data to Googlesheet",
  "icons": {
    "16": "./resource/assets/icons/ext-icon.png",
    "64": "./resource/assets/icons/ext-icon.png",
    "128": "./resource/assets/icons/ext-icon.png"
  },
  "manifest_version": 3,
  "name": "Linkedin Hugger",
  "permissions": ["tabs", "activeTab"],
  "version": "0.1.5",
  "host_permissions": [
    "https://www.linkedin.com/in/*",
    "https://www.linkedin.com/company/*"
  ]
}